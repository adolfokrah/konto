{
  "name": "konto",
  "version": "4.4.9",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "build:vercel": "pnpm install --no-frozen-lockfile && cross-env NODE_OPTIONS=--no-deprecation next build",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "dev": "infisical run --env=dev -- cross-env NODE_OPTIONS=--no-deprecation next dev",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm build && pnpm start",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test": "pnpm run test:int && pnpm run test:e2e",
    "test:e2e": "infisical run --env=dev -- cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pnpm exec playwright test --config=playwright.config.ts",
    "test:int": "infisical run --env=dev -- cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts",
    "test:int:ci": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts"
  },
  "dependencies": {
    "@payloadcms/admin-bar": "3.55.1",
    "@payloadcms/db-mongodb": "3.55.1",
    "@payloadcms/live-preview-react": "3.55.1",
    "@payloadcms/next": "3.55.1",
    "@payloadcms/payload-cloud": "3.55.1",
    "@payloadcms/plugin-form-builder": "3.55.1",
    "@payloadcms/plugin-nested-docs": "3.55.1",
    "@payloadcms/plugin-redirects": "3.55.1",
    "@payloadcms/plugin-search": "3.55.1",
    "@payloadcms/plugin-seo": "3.55.1",
    "@payloadcms/richtext-lexical": "3.55.1",
    "@payloadcms/storage-uploadthing": "^3.55.1",
    "@payloadcms/ui": "3.55.1",
    "@paystack/inline-js": "^2.22.7",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@sentry/nextjs": "^10.11.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^10.0.0",
    "dotenv": "17.2.2",
    "geist": "^1.5.1",
    "graphql": "^16.11.0",
    "lucide-react": "^0.544.0",
    "next": "15.5.3",
    "next-sitemap": "^4.2.3",
    "next-themes": "^0.4.6",
    "payload": "3.55.1",
    "prism-react-renderer": "^2.4.1",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-hook-form": "7.62.0",
    "require-in-the-middle": "7.5.2",
    "sharp": "0.34.3",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@playwright/test": "1.55.0",
    "@tailwindcss/postcss": "^4.1.13",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/react": "16.3.0",
    "@types/escape-html": "^1.0.4",
    "@types/node": "24.3.3",
    "@types/paystack__inline-js": "^1.0.2",
    "@types/react": "19.1.13",
    "@types/react-dom": "19.1.9",
    "@vitejs/plugin-react": "5.0.2",
    "autoprefixer": "^10.4.21",
    "copyfiles": "^2.4.1",
    "eslint": "^9.35.0",
    "eslint-config-next": "15.5.3",
    "jsdom": "26.1.0",
    "playwright": "1.55.0",
    "playwright-core": "1.55.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.13",
    "typescript": "5.9.2",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "esbuild",
      "unrs-resolver"
    ],
    "overrides": {
      "webidl-conversions": "^7.0.0",
      "whatwg-url": "^11.0.0"
    }
  }
}
